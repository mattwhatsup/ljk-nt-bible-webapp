{
  "name": "vite-template-redux",
  "homepage": "https://mattwhatsup.github.io/ljk-nt-bible-webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "preview": "vite preview",
    "test": "vitest run",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "preprocess": "npx tsx src/scripts/preprocess.ts",
    "build": "tsc && vite build",
    "gh-build": "cross-env NODE_ENV=gh tsc && cross-env NODE_ENV=gh vite build && cross-env NODE_ENV=gh node src/scripts/copy-index.js",
    "predeploy": "npm run gh-build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@babel/standalone": "^7.26.8",
    "@chakra-ui/react": "^3.8.0",
    "@emotion/react": "^11.14.0",
    "@headlessui/react": "^1.7.14",
    "@reduxjs/toolkit": "^2.0.1",
    "@tailwindcss/vite": "^4.0.5",
    "axios": "^1.7.9",
    "next-themes": "^0.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.4.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.29.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/babel__standalone": "^7.1.9",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "gh-pages": "^6.3.0",
    "jsdom": "^23.2.0",
    "postcss": "^8.5.1",
    "prettier": "^3.2.1",
    "tailwindcss": "^4.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.2.0"
  }
